<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from testWSService WSDL service -->

  <service name="testWS" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://imie.org"/>
      <parameter name="wsdlServiceElement" value="testWSService"/>
      <parameter name="schemaQualified" value="http://imie.org"/>
      <parameter name="wsdlServicePort" value="testWS"/>
      <parameter name="className" value="org.imie.TestWSSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="testWS"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="methode" qname="operNS:methode" xmlns:operNS="http://imie.org" returnQName="retNS:methodeReturn" xmlns:retNS="http://imie.org" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
      </operation>
      <parameter name="allowedMethods" value="methode"/>

  </service>
</deployment>
